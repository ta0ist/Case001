<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<meta name="description" content="">
	<meta name="author" content="">
	<link rel="shortcut icon" href="./images/favicon.png">

	<title>Clean Zone</title>

	<!-- Bootstrap core CSS -->
	<link href="./javascripts/bootstrap/dist/css/bootstrap.css" rel="stylesheet">
	<link rel="stylesheet" type="text/css" href="./javascripts/jquery.gritter/css/jquery.gritter.css" />

	<link rel="stylesheet" href="./fonts/font-awesome-4/css/font-awesome.min.css">

	<!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
	<!--[if lt IE 9]>
	  <script src="../../assets/js/html5shiv.js"></script>
	  <script src="../../assets/js/respond.min.js"></script>
	<![endif]-->
	<link rel="stylesheet" type="text/css" href="./javascripts/jquery.nanoscroller/nanoscroller.css" />
	<link rel="stylesheet" type="text/css" href="./javascripts/jquery.easypiechart/jquery.easy-pie-chart.css" />
	<link rel="stylesheet" type="text/css" href="./javascripts/bootstrap.switch/bootstrap-switch.css" />
	<link rel="stylesheet" type="text/css" href="./javascripts/bootstrap.datetimepicker/css/bootstrap-datetimepicker.min.css" />
	<link rel="stylesheet" type="text/css" href="./javascripts/jquery.select2/select2.css" />
	<link rel="stylesheet" type="text/css" href="./javascripts/bootstrap.slider/css/slider.css" />
	<!-- Custom styles for this template -->
	<link href="./stylesheets/style.css" rel="stylesheet" />	

</head>

<body class="texture" ng-app='mybody' ng-controller='myctrl'>
<div class="row">
<div id="cl-wrapper" class="login-container">

   <div class="middle-login">
		<div class="block-flat">
			<div class="header">							
				<h3 class="text-center"><img class="logo-img" src="./images/logo.png" alt="logo"/>Clean Zone</h3>
			</div>
			<div>
				<form style="margin-bottom: 0px !important;" class="form-horizontal" >
					<div class="content">
						<h4 class="title"><%=title%></h4>
							<div class="form-group">
								<div class="col-sm-12">
									<div class="input-group">
										<span class="input-group-addon"><i class="fa fa-user"></i></span>
										<input type="text" placeholder="Username" id="username" class="form-control" ng-model='checkUser'>
									</div>
								</div>
							</div>
							<div class="form-group">
								<div class="col-sm-12">
									<div class="input-group">
										<span class="input-group-addon"><i class="fa fa-lock"></i></span>
										<input type="password" placeholder="Password" id="password" class="form-control" ng-model='checkPWD'>
									</div>
								</div>
							</div>
							
					</div>
					<div class="foot spacer2">
						<button data-toggle="modal" data-target="#mod-success" type="button" class="btn btn-primary btn-rad">注册</button>
 
						<button class="btn btn-primary" data-dismiss="modal" type="submit" id="login" ng-click="login(this)">登录</button>
					</div>
				</form>
			</div>
		</div>      
    </div>
    </div>
</div>
</div>
<div>
    <table >
        <tr><td>用户名</td><td>密码</td></tr>
        <tr ng-repeat='user in userList'>
        <td>{{user.name}}</td>
        <td>{{user.password}}</td>
        </tr>
    </table>
</div>
              <div class="modal fade" id="mod-success" tabindex="-1" role="dialog">
                <div class="modal-dialog">
                  <div class="modal-content">
                  <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                  </div>
                  <div class="modal-body">
                    <div class="text-center">
                     <h4>注册信息</h4>
                      <div>
                       <label>用户名</label>
                       <input type="text" name="" ng-model='user.name'>
                      </div>
                      <div>
                       <label>密码</label>
                       <input type="text" name="" ng-model='user.password'>
                      </div>
                    </div>
                  </div>
                  <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">取消</button>
                    <button type="button" class="btn btn-success" data-dismiss="modal" ng-click='add(this)'>确认</button>
                  </div>
                  </div><!-- /.modal-content -->
                </div><!-- /.modal-dialog -->
                </div><!-- /.modal -->
	<script src="./javascripts/jquery.js"></script>
  <!-- <script src="js/echarts.js"></script> -->
	<script type="text/javascript" src="./javascripts/jquery.nanoscroller/jquery.nanoscroller.js"></script>
	<script type="text/javascript" src="./javascripts/jquery.sparkline/jquery.sparkline.min.js"></script>
	<script type="text/javascript" src="./javascripts/jquery.easypiechart/jquery.easy-pie-chart.js"></script>
	<script type="text/javascript" src="./javascripts/behaviour/general.js"></script>
    <script src="./javascripts/jquery.ui/jquery-ui.js" type="text/javascript"></script>
	<script type="text/javascript" src="./javascripts/jquery.nestable/jquery.nestable.js"></script>
	<script type="text/javascript" src="./javascripts/bootstrap.switch/bootstrap-switch.min.js"></script>
	<script type="text/javascript" src="./javascripts/bootstrap.datetimepicker/js/bootstrap-datetimepicker.min.js"></script>
	<script src="./javascripts/jquery.select2/select2.min.js" type="text/javascript"></script>
	<script src="./javascripts/bootstrap.slider/js/bootstrap-slider.js" type="text/javascript"></script>
	<script type="text/javascript" src="./javascripts/jquery.gritter/js/jquery.gritter.js"></script>
  <script type="text/javascript" src="./javascripts/angular.min.js"></script>
  <script type="text/javascript" src="./javascripts/jquery.niftymodals/js/jquery.modalEffects.js"></script>
  <script type="text/javascript">
    angular.module("mybody",[]).controller('myctrl',['$scope','$http',function($scope,$http){
      App.init();
			$('.md-trigger').modalEffects();
      $scope.userList=[];
      $scope.user={
        name:"",
        password:""
      };

      $scope.checkUser="";
      $scope.checkPWD="";
      var check = false;

      $scope.add=function(){
        var newObject=$.extend({},$scope.user);
        $scope.userList.push(newObject);
      }

      $scope.login = function(e){
      	 $http.get('/menu').success(function(data){
						// var da = JSON.parse(data);
      	 		alert(data[0].MENU_NAME);
      	 })
      }
    }])
  </script>

<!-- Bootstrap core JavaScript
================================================== -->
<!-- Placed at the end of the document so the pages load faster -->
  <script src="./javascripts/behaviour/voice-commands.js"></script>
  <script src="./javascripts/bootstrap/dist/js/bootstrap.min.js"></script>
<script type="text/javascript" src="./javascripts/jquery.flot/jquery.flot.js"></script>
<script type="text/javascript" src="./javascripts/jquery.flot/jquery.flot.pie.js"></script>
<script type="text/javascript" src="./javascripts/jquery.flot/jquery.flot.resize.js"></script>
<script type="text/javascript" src="./javascripts/jquery.flot/jquery.flot.labels.js"></script>
</body>
</html>

